<mapper namespace="all.about.apartment.mappers.UserMapper">

	<select id="login" resultType="ResidentVO" parameterType="LoginDTO">
		SELECT * FROM RESIDENT
		WHERE R_ID = #{urd}
		AND R_PW = #{upw}
	</select>
